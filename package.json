  "license": "MIT",
  "scripts": {
    "eslint": "eslint src",
    "client-dist": "NODE_ENV=production webpack --progress ",
    "client-dev": "webpack-dev-server --hot --inline --host 0.0.0.0 --port 8087",
    "client-dev1": "webpack serve --hot ",
    "srv-dev": "DEBUG=tetris:* babel-watch -w src src/server/main.js",
